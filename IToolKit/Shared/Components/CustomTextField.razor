@inherits MudTextField<string>
@inject IJSRuntime JSRuntime

@if (ReadOnly)
{
    <MudStack Row Class="mb-n1" Spacing="1" Justify="Justify.FlexEnd">
            <MudButton StartIcon="@Icons.Material.Filled.QrCode" IconSize="Size.Large" Size="Size.Small" Variant="Variant.Filled" OnClick="GenerateQRCode">QR Code</MudButton>
            <MudButton StartIcon="@Icons.Material.Filled.ContentCopy" IconSize="Size.Large" Size="Size.Small" Variant="Variant.Filled" OnClick="Copy">Copy</MudButton>
    </MudStack>
}

@{
    base.BuildRenderTree(__builder);
}